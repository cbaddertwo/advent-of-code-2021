file = open ('C:\\Users\\12268\\Desktop\\AdventOfCode\\day 1\\input.txt', 'r')

content = file.readlines()

count = 0

lastline = 10000;

for line in content:
    for i in line:
        #print(line)
        if int(lastline)<int(line):
            count = count + 1
            
        lastline = line;

print (count)
